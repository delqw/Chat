{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": ["plugin:vue/recommended"],
  "plugins": ["vue"],
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error", "info", "dir"] }]
  }
}